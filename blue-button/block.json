{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "customberg/blue-button",
	"version": "0.1.0",
	"title": "Blue Button",
	"category": "customberg",
	"icon": "button",
	"description": "Blue button block.",
	"attributes": {
		"buttonText": {
			"type": "string",
			"default": "Request Information"
		},
		"align": {
			"type": "string",
			"default": "left"
		},
		"post": {
			"type": "object",
			"default": {
				"url": ""
			}
		}
	},
	"supports": {
		"html": false,
		"align": [
			"left",
			"center",
			"right"
		]
	},
	"textdomain": "customberg",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
