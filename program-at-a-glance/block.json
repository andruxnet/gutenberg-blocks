{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "customberg/program-at-a-glance",
	"version": "0.1.0",
	"title": "Program at a Glance",
	"category": "customberg",
	"icon": "editor-table",
	"description": "Brief summary of program details for potential students.",
	"attributes": {
		"title": {
			"type": "string"
		},
		"subtitle": {
			"type": "string"
		},
		"disclaimer": {
			"type": "string"
		},
		"additionalDisclaimer": {
			"type": "string"
		}
	},
	"supports": {
		"html": false,
		"color": {
			"gradients": true,
			"link": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		},
		"spacing": {
			"margin": [ "top", "bottom" ],
			"padding": true,
			"blockGap": true,
			"__experimentalDefaultControls": {
				"padding": true,
				"blockGap": true
			}
		}
	},
	"textdomain": "customberg",
	"script": "file:./front.js",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
